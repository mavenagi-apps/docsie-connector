{
  "name": "docsie-connector",
  "version": "1.0.0",
  "description": "Docsie knowledge connector for Maven AGI",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "vitest",
    "test:run": "vitest run",
    "validate": "tsx src/validate.ts",
    "sync": "tsx src/index.ts"
  },
  "keywords": [
    "mavenagi",
    "docsie",
    "knowledge-connector"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bottleneck": "^2.19.5",
    "dotenv": "^16.4.5",
    "mavenagi": "^1.0.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "eslint": "^9.16.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^2.1.6"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@9.14.2"
}
